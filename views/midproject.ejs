    <section id="portfolio"  class="section-bg" >
      <div class="container">

        <div class="row portfolio-container">
          <% for(let i=0;i < data.length;i++){ %>
            <div class="col-lg-4 col-md-6 portfolio-item filter-app wow fadeInUp">
              <div class="portfolio-wrap">
                <figure>
                  <img src="/img/tku/<%= data[i].pic %>" class="img-fluid" alt="">
                  <a href="/img/tku/<%= data[i].pic %>" data-lightbox="portfolio" data-title="App 1" class="link-preview" title="Preview"><i class="ion ion-eye"></i></a>
                  <a href="#" class="link-details" title="More Details"><i class="ion ion-android-open"></i></a>
                </figure>

                <div class="portfolio-info">
                  <h4><a href="/<%= data[i].link %>"><%= data[i].name %></a></h4>
                </div>
              </div>
            </div>
          <% } %>
        </div>

      </div>
    </section><!-- #portfolio -->

